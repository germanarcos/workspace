	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String entrada;
		Scanner teclado = new Scanner(System.in);
		System.out.println("Introduce una cadena");
		entrada = teclado.nextLine();
		
		teclado.close();
	}


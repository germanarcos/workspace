
public class Ejercicio10 {
	public static void ejercicio10(){
		int NUM_CARTAS = 40;
		int[] cartas = new int[NUM_CARTAS];
		for(int i=0;i<NUM_CARTAS;i++){
			switch(i%10){
			case 0: System.out.println("1");
			break;
			case 1: System.out.println("2");
			break;
			case 2: System.out.println("3");
			break;
			case 3: System.out.println("4");
			break;
			case 4: System.out.println("5");
			break;
			case 5: System.out.println("6");
			break;
			case 6: System.out.println("7");
			break;
			case 7: System.out.println("Sota");
			break;
			case 8: System.out.println("Caballo");
			break;
			case 9: System.out.println("Rey");
			break;
			
			}
		}

	}
}

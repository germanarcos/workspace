import java.util.LinkedList;
public class Clicker {
	public static void clicker(){
		LinkedList<pulsacion> clicks = new LinkedList<pulsacion>();
		LinkedList tiempo = new LinkedList();			
		pulsacion p = new pulsacion();
		p.pulsa();

		//pulsaciones
		do{
			pulsacion p = new pulsacion();
			p.pulsa();
			clicks.add(p);
			
		}while(p.comprueba!='q');
	}

}
